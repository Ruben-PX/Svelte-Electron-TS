{
  "name": "app",
  "productName": "App",
  "version": "0.1.0",
  "description": "My Electron application description",
  "main": "build/main.js",
  "scripts": {
    "start:v2": "cross-env NODE_ENV=development npm-run-all -p -r build:frontend:dev start:electron",
    "build:frontend:dev": "cross-env NODE_ENV=development rollup -c -w",
    "start:electron": "npm-run-all -p build:electron:dev:watch start:electron:dev",
    "build:electron:dev:watch": "tsc -w --p ./tsconfig.electron.json",
    "start:electron:dev": "npm-run-all -s build:electron:dev start:electron:nodemon",
    "build:electron:dev": "tsc --p ./tsconfig.electron.json",
    "start:electron:nodemon": "nodemon",
    "start:electron:run": "electron .",
    "start": "ts-node scripts/start.ts",
    "build": "ts-node scripts/build.ts",
    "lint": "ts-node scripts/lint.ts",
    "format": "prettier --write ."
  },
  "repository": "https://github.com/raphtlw/electron-svelte-ts",
  "author": "RubenPX",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.3",
    "electron-reloader": "^1.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "shelljs": "^0.8.4",
    "svelte": "^3.32.2",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-node": "^9.1.0",
    "tslib": "^2.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {}
}
